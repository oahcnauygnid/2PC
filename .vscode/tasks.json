{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: g++-8 生成活动文件",
            "command": "/usr/bin/g++-8",
            "args": [
                "-std=c++17",
                "-fdiagnostics-color=always",
                "-g",
                "${file}",
                "-L'/usr/local/lib'",
                "-lseal-3.7",
                "-o",
                "${workspaceRoot}/bin/${fileBasenameNoExtension}"
            ],
            "options": {
                "cwd": "${workspaceRoot}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "调试器生成的任务。"
        }
    ],
    "version": "2.0.0"
}